<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<!-- 修改学校名称 -->
<suite name="【阳江职业技术学院】视频播放" parallel="tests" preserve-order="true"
	thread-count="1">
	<listeners>
		<listener class-name="com.kkb.test.util.reporter.HTMLReporter" />
		<listener class-name="com.kkb.test.util.reporter.JUnitXMLReporter" />
		<listener class-name="com.kkb.test.util.reporter.TestngListener" />
	</listeners>
	
	
	<!-- 修改下面三处，依次为学校首页，用户名 密码 -->
	<parameter name="baseUrl" value="http://yjcollege.gaoxiaobang.com/" />
	<parameter name="loginName" value="gxb0000" />
	<parameter name="password" value="123456" />
	
	<!--配置课程-->
	
	
	<test name="自我发展与创业指导">
		<parameter name="courseName" value="自我发展与创业指导" />
		<classes>
			<class name="com.kkb.test.steps.pc.gxb.CoursePlayStep" />
		</classes>
	</test>
	
	<test name="职业素养">
		<parameter name="courseName" value="职业素养" />
		<classes>
			<class name="com.kkb.test.steps.pc.gxb.CoursePlayStep" />
		</classes>
	</test>

</suite>


