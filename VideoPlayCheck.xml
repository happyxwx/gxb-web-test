<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<!-- 修改学校名称 -->
<suite name="山东外贸职业学院" parallel="tests" preserve-order="true"
	   thread-count="1">
	<listeners>
		<listener class-name="com.kkb.test.util.reporter.HTMLReporter" />
		<listener class-name="com.kkb.test.util.reporter.JUnitXMLReporter" />
		<listener class-name="com.kkb.test.util.reporter.TestngListener" />
	</listeners>


	<!-- 修改下面三处，依次为学校首页，用户名 密码 -->

	<parameter name="baseUrl" value="http://sdwm.gaoxiaobang.com/" />
	<parameter name="loginName" value="gxb0000" />
	<parameter name="password" value="123456" />
	<!--配置课程-->
	<test name="互联网营销概论">
		<parameter name="courseName" value="互联网营销概论"/>
		<classes>
			<class name="com.kkb.test.steps.pc.gxb.CoursePlayStep"/>
		</classes>
	</test>
	<test name="搜索引擎、微信与口碑营销">
		<parameter name="courseName" value="搜索引擎、微信与口碑营销"/>
		<classes>
			<class name="com.kkb.test.steps.pc.gxb.CoursePlayStep"/>
		</classes>
	</test>
</suite>
