<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<!-- 修改学校名称 -->
<suite name="复旦大学软件学院在线学习平台" parallel="tests" preserve-order="true"
	thread-count="1">
	<listeners>
		<listener class-name="com.kkb.test.util.reporter.HTMLReporter" />
		<listener class-name="com.kkb.test.util.reporter.JUnitXMLReporter" />
		<listener class-name="com.kkb.test.util.reporter.TestngListener" />
	</listeners>
	
	
	<!-- 修改下面三处，依次为学校首页，用户名 密码 -->
	<parameter name="baseUrl" value="http://fudan.gaoxiaobang.com/" />
	<parameter name="loginName" value="13110240020" />
	<parameter name="password" value="13110240020" />



	<!-- 课程名称1 按照下列格式，修改2处课程名称即可，例如修改为‘计算机文化基础【诚毅学院】’， -->
	<test name="计算思维">
		<parameter name="courseName" value="计算思维" />
		<classes>
			<class name="com.kkb.test.steps.pc.gxb.gcu.CoursePlayStep" />
		</classes>
	</test>
	
    <!-- 课程名称2 后面依次类推 ，添加课程复制即可-->
	<test name="计算机数据库基础与应用">
		<parameter name="courseName" value="计算机数据库基础与应用" />
		<classes>
			<class name="com.kkb.test.steps.pc.gxb.gcu.CoursePlayStep" />
		</classes>
	</test>
	
		<test name="计算机多媒体应用基础">
		<parameter name="courseName" value="计算机多媒体应用基础" />
		<classes>
			<class name="com.kkb.test.steps.pc.gxb.gcu.CoursePlayStep" />
		</classes>
	</test>

	

</suite>


